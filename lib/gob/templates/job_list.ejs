<ol class="breadcrumb">
  <li><a href="/">Apps</a></li>
  <li><a href="/apps/<%= app.attributes.name %>/"><%= app.attributes.name %></a></li>
  <li class="active">Jobs</li>
</ol>
<a id="new-job" class="btn btn-primary"
   href="/apps/<%= app.attributes.name %>/jobs/new">New job</a>

<% _.each(models, function(item) { %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= item.attributes.name %>
      <p><%= item.attributes.description %></p>
    </div>
  </div>
<% }) %>
